{
  "name": "storylunch",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "fire": "run-s tab:dev",
    "firem": "npm run kill && npm run reset-algolia && run-s tab:emulators tab:devm tab:emulators-open",
    "tab:emulators": "ttab -t 'emulators' npm run emulators",
    "tab:dev": "ttab -t 'dev' npm run dev",
    "tab:devm": "ttab -t 'dev' FIREBASE_ENV=emulators npm run dev",
    "tab:emulators-open": "open http://localhost:4000",
    "emulators": "firebase emulators:start",
    "kill": "lsof -ti tcp:8080 | xargs kill",
    "dev": "nuxt -o",
    "build": "nuxt build",
    "start": "nuxt start",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint": "npm run lint:js",
    "test": "jest",
    "hp": "git push heroku feature/auth:main",
    "heroku-postbuild": "npm run build",
    "resetm": "npm run reset-emulator-auth && npm run reset-emulator-db && npm run reset-algolia",
    "reset-emulator-auth": "curl -H 'Authorization: Bearer owner' -X DELETE http://localhost:9099/emulator/v1/projects/storylunch/accounts",
    "reset-emulator-db": "FIRESTORE_EMULATOR_HOST=localhost:8080 firebase --debug --project=storylunch firestore:delete --recursive --all-collections",
    "resetc": "npm run reset-auth && npm run reset-storage && npm run reset-db && npm run reset-algolia",
    "reset-auth": "node ./utils/firebaseDeleteAllUsers.js",
    "reset-storage": "node ./utils/firebaseDeleteAllStorage.js",
    "reset-db": "firebase firestore:delete --all-collections -y",
    "reset-algolia": "node ./utils/algoliaClearUserIndex.js",
    "set-users-m": "curl http://localhost:12345/storylunch/europe-west1/populateAuthUsers",
    "set-users-c": "curl https://europe-west1-storylunch.cloudfunctions.net/populateAuthUsers",
    "firedf": "firebase deploy --only functions"
  },
  "dependencies": {
    "@braid/vue-formulate": "^2.4.5",
    "@nuxt/content": "^1.9.0",
    "@nuxtjs/axios": "^5.12.2",
    "@nuxtjs/cloudinary": "^1.0.2",
    "@nuxtjs/firebase": "^7.3.0",
    "@nuxtjs/pwa": "^3.2.2",
    "@nuxtjs/recaptcha": "^0.6.2",
    "algoliasearch": "^4.8.3",
    "core-js": "^3.6.5",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "firebase": "^8.1.1",
    "helmet": "^4.2.0",
    "instantsearch.css": "^7.4.5",
    "md5": "^2.3.0",
    "morgan": "^1.10.0",
    "node-fetch": "^2.6.1",
    "nuxt": "^2.14.6",
    "v-click-outside": "^3.1.2",
    "vue-instantsearch": "^3.4.2",
    "vue-js-modal": "^2.0.0-rc.6",
    "vuex-persistedstate": "^4.0.0-beta.1"
  },
  "devDependencies": {
    "@nuxt/image": "0.0.4",
    "@nuxt/types": "^2.14.7",
    "@nuxtjs/eslint-config": "^3.1.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "@nuxtjs/svg": "^0.1.12",
    "@nuxtjs/tailwindcss": "^3.1.0",
    "@vue/test-utils": "^1.1.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.5.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-nuxt": "^1.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "firebase-admin": "^9.4.1",
    "http-server": "^0.12.3",
    "jest": "^26.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "ttab": "^0.7.0",
    "vue-jest": "^3.0.4"
  }
}
